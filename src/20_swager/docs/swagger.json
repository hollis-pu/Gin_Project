            }
        },
        "controller._ResponseData": {
            "type": "object",
            "properties": {
                "code": {
                    "description": "状态码",
                    "type": "integer"
                },
                "data": {
                    "description": "数据"
                },
                "msg": {
                    "description": "状态信息",
                    "type": "string"
                }
            }
        }
    }
